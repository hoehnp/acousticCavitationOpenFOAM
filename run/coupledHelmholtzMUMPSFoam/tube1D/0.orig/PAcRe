FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      PAcRe;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    transducer
    {
        type            fixedGradient;
        gradient        uniform 142e+6;
    }

    freeSurface
    {
        type            fixedValue;
        value           uniform 0;
    }
    
    walls
    {
        type            fixedValue;
        value           uniform 0;
    }
}

